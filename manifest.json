{
	"name" : "HTML clean",
	"version" : "1.0",
	"author" : "Winston Marvel",
	"manifest_version" : 2,
	"description": "Clean HTML from inline CSS/inline scripts/classes/unecessary div tags",
	"browser_action":{
		"default_icon": "icon.png",
		"default_popup" : "popup.html"
	},
	"icons":{
      "128": "icon128.png"
   },
	"permissions" : [  "contextMenus" ],
	"background" :{
		"scripts": ["background.html"]
	}
}